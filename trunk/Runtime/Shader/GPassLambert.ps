#include "G_PSBase.sh"


//--------------------------------------------------------------------------------------
// IO structures
//--------------------------------------------------------------------------------------
struct PS_INPUT
{
    float4 Pos : SV_POSITION;
	float2 Tex : TEXCOORD0; 
    float3 ViewNormal : TEXCOORD1;
    float Depth : TEXCOORD2;	
};

struct PS_OUTPUT
{
	float4 Normals	: SV_Target0;
	float4 Depth	: SV_Target1;
};

//--------------------------------------------------------------------------------------
// Pixel Shader
//--------------------------------------------------------------------------------------
float4 PS( PS_INPUT In) : SV_Target
{
	return float4(1,1,1,1);
}
